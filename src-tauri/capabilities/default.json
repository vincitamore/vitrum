{
  "$schema": "https://schemas.tauri.app/config/2/capability",
  "identifier": "default",
  "description": "Default capabilities for org-viewer",
  "windows": ["main"],
  "permissions": [
    "core:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "http://127.0.0.1:3847/*" },
        { "url": "http://localhost:3847/*" }
      ]
    }
  ]
}
